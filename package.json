{
  "name": "reskue-api",
  "version": "0.0.1",
  "private": true,
  "description": "Reskue APIs",
  "license": "UNLICENSED",
  "author": "Tom Vacherat",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:parser": "nest build parser-sls",
    "depcheck": "depcheck --config ./depcheck-config.json",
    "deploy": "nest build && npm run migrate && npm run start:prod",
    "deploy:parser": "nest build parser-sls && sls deploy",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && npm run format:check",
    "migrate": "rm -rf libs/database/migrations && npx mikro-orm migration:create && npx mikro-orm migration:up",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prepush": "npm run build && npm run test --passWithNoTests",
    "seed": "npx mikro-orm seeder:run",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch ",
    "start:prod": "node dist/apps/api/main",
    "test": "jest -i --passWithNoTests",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@api(|/.*)$": "<rootDir>/apps/api/src/$1",
      "^@app/aws(|/.*)$": "<rootDir>/libs/aws/src/$1",
      "^@app/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^@app/storage(|/.*)$": "<rootDir>/libs/storage/src/$1",
      "^@app/authentication(|/.*)$": "<rootDir>/libs/authentication/src/$1",
      "^@app/mailing(|/.*)$": "<rootDir>/libs/mailing/src/$1",
      "^@app/sse(|/.*)$": "<rootDir>/libs/sse/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@app/socket-io(|/.*)$": "<rootDir>/libs/socket-io/src/$1",
      "^@app/notifications(|/.*)$": "<rootDir>/libs/notifications/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1"
    },
    "rootDir": ".",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "@aws-sdk/client-comprehend": "^3.484.0",
    "@aws-sdk/client-dynamodb": "^3.484.0",
    "@aws-sdk/client-lambda": "^3.484.0",
    "@aws-sdk/client-route-53": "^3.484.0",
    "@aws-sdk/client-s3": "^3.484.0",
    "@aws-sdk/client-secrets-manager": "^3.484.0",
    "@aws-sdk/client-sqs": "^3.484.0",
    "@medibloc/nestjs-request-context": "^1.0.1",
    "@mikro-orm/cli": "^5.9.7",
    "@mikro-orm/core": "^5.9.7",
    "@mikro-orm/migrations": "^5.9.7",
    "@mikro-orm/nestjs": "5.2.3",
    "@mikro-orm/postgresql": "^5.9.7",
    "@mikro-orm/seeder": "^5.9.7",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/websockets": "^10.3.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "@swc/core": "^1.3.102",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "20.10.6",
    "@types/passport-google-oauth2": "^0.1.8",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.5",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^7.1.1",
    "dotenv": "^16.3.1",
    "emoji-regex": "^10.3.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "file-type": "^16.5.4",
    "gaxios": "^6.1.1",
    "graphql": "^16.8.1",
    "husky": "^8.0.3",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-linkedin-oauth2": "github:auth0/passport-linkedin-oauth2#v3.0.0",
    "passport-local": "^1.0.0",
    "postmark": "^4.0.2",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rword": "^3.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "sift": "^17.0.1",
    "socket.io": "^4.7.2",
    "sqs-consumer": "^8.1.3",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@automock/jest": "^2.1.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@swc/cli": "^0.1.63",
    "@swc/jest": "^0.2.29",
    "@types/express-session": "^1.17.10",
    "@types/jest": "29.5.11",
    "@types/passport-linkedin-oauth2": "^1.5.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "run-script-webpack-plugin": "^0.2.0",
    "serverless-offline": "^13.3.2",
    "serverless-plugin-include-dependencies": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "libs/database/mikro-orm.config.ts"
    ]
  }
}
